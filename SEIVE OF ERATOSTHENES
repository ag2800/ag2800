#include<bits/stdc++.h>
using namespace std;

int N=1000000;
bool seive[1000001];
 vector<int> v;
void createseive(){
   
    for(int i=2;i<=N;i++){
        seive[i]=true;
    }
    for(int i=2;i*i<=N;i++){
        if(seive[i]==true){
            for(int j=i*i;j<=N;j=j+i){
                seive[j]=false;
                
            }
        }
    }
    for(int i=2;i<=N;i++){
        if(seive[i]==true){
            v.push_back(i);
        }
    }
    
}





int main() {
	// your code goes here
//seive of eratosthenes;
createseive();
    int t;
    cin>>t;
    while(t--){
        int k;
        cin>>k;
       for(int i=0;i<k;i++){
           cout<<v[i]<<" ";
       }
    }
     
	return 0;
}
